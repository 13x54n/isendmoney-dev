{
  "expo": {
    "name": "iSendMoney",
    "slug": "isendmoney",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "isendmoney",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.minginc.isendmoney",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-video",
      "@react-native-google-signin/google-signin"
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*.mov"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2104e755-7682-4292-a81a-3d825b99f4e2"
      }
    },
    "android": {
      "package": "xyz.minginc.isendmoney"
    }
  }
}
